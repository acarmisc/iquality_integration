<openerp>
    <record id="view_scheduler_iquality_form" model="ir.ui.view">
        <field name="name">scheduler.iquality.form</field>
        <field name="model">scheduler.iquality</field>
        <field name="arch" type="xml">
            <form string="Schedule iquality Sync">
            <group>
                <field name="name"/>
                <field name="num_of_updates"/>
                <field name="last_modified"/>
            </group>
            </form>
        </field>
    </record>

    <!-- tree (list) view-->
    <record id="view_scheduler_iquality_tree" model="ir.ui.view">
        <field name="name">scheduler.iquality.tree</field>
        <field name="model">scheduler.iquality</field>
        <field name="arch" type="xml">
            <tree string="Schedule iquality Sync">
            <field name="name"/>
            <field name="num_of_updates"/>
            <field name="last_modified"/>
            </tree>
        </field>
    </record>
</openerp>